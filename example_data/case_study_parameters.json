{
    "files": {
        "peaktable": {
            "filepath": "example_data/case_study_peak_table_quant_full.csv",
            "format": "mzmine3",
            "polarity": "positive"
        },
        "msms": {
            "filepath": "example_data/case_study_MSMS.mgf",
            "format": "mgf"
        },
        "phenotype": {
            "filepath": "example_data/case_study_bioactivity.csv",
            "format": "fermo"
        },
        "group_metadata": {
            "filepath": "example_data/case_study_group_metadata.csv",
            "format": "fermo"
        },
        "spectral_library": {
            "filepath": "example_data/case_study_spectral_library.mgf",
            "format": "mgf"
        }
    },
    "core_modules": {
        "adduct_annotation": {
            "activate_module": true,
            "mass_dev_ppm": 20.0
        },
        "spec_sim_networking": {
            "activate_module": true,
            "algorithm": [
                {
                    "name": "modified_cosine",
                    "msms_min_frag_nr": 5,
                    "fragment_tol": 0.1,
                    "min_nr_matched_peaks": 5,
                    "score_cutoff": 0.7,
                    "max_nr_links": 10
                },
                {
                    "name": "ms2deepscore",
                    "directory_path": "/path/to/dir",
                    "score_cutoff": 0.7,
                    "max_nr_links": 10
                }
            ]
        }
    },
    "additional_modules": {
        "peaktable_filtering": {
            "filter_rel_int_range": [
                0.0,
                1.0
            ]
        },
        "blank_assignment": {
            "activate_module": true,
            "column_ret_fold": 10
        },
        "phenotype_assignment": {
            "activate_module": true,
            "algorithm": [
                {
                    "name": "fold_difference",
                    "fold_diff": 10,
                    "data_type": "percentage"
                }
            ]
        },
        "spectral_library_matching": {
            "activate_module": true,
            "algorithm": [
                {
                    "name": "modified_cosine",
                    "fragment_tol": 0.1,
                    "min_nr_matched_peaks": 5,
                    "score_cutoff": 0.7
                },
                {
                    "name": "ms2deepscore",
                    "directory_path": "/path/to/dir",
                    "score_cutoff": 0.7
                }
            ]
        },
        "ms2query_annotation": {
            "activate_module": true,
            "directory_path": "/path/to/dir",
            "consider_blank": true,
            "filter_rel_int_range": [
                0.0,
                0.1
            ]
        }
    }
}